html {
background-color: #000 !important;
}

body {
background-color: #000 !important;
color: green !important;
}

ul {
background-color: #000 !important;
}

tr, td, th {
background-color: #000 !important;
}

.navbar {
background-color: #000 !important;
}

a {
color: #0f0 !important;
}

a:hover {
color: #009100 !important;
}

.post bg1 unreadpost {
background-color: #2f2f2f !important;
}

.post bg2 unreadpost{
background-color: #464646 !important;
}

.bg1 {
background-color: #222 !important;
border: 1px solid white;
box-shadow: 0px 0px 10px 2px rgb(153, 153, 153);
margin-bottom: 20px !important;
}

.bg2 {
background-color: #222 !important;
border: 1px solid white;
box-shadow: 0px 0px 10px 2px rgb(153, 153, 153);
margin-bottom: 20px !important;
}

dt, dd {
color: #ada69c !important;
}

li.row:hover {
background-color: unset !important;
}

.row.bg1 {
background-color: #000 !important;
}

.row.bg2 {
background-color: #000 !important;
}

ul.profile-icons {
background: none !important;
}

.headerbar {
background-color: black;
filter: grayscale(1);
}

p {
color: #999 !important;
}

.signature {
color: #999 !important;
}

.panel {
background-color: #555 !important;
color: black !important;
border: 1px solid #999;
box-shadow: 0px 0px 10px #aaa;
}

blockquote {
background-color: #555;
border-color: #b7b7b7;
}

blockquote blockquote {
background-color: #999;
}

.pagination span a:hover {
border-color: #999 !important;
background-color: #333 !important;
color: #FFF !important;
}
.pagination span a, .pagination span a:link {
background-color: #bbb !important;
border-color: #eee !important;
}

.pagination span strong {
color: #FFFFFF;
background-color: #555;
border-color: #aaa;
}

.copyright {
color: #999;
}

label {
color: #999 !important;
}

.content h2, .panel h2 {
color: #14ff00;
}

span.corners-top, span.corners-bottom, span.corners-top span, span.corners-bottom span {
background-image: none !important;
}

.forumbg {
background-color: black !important;
}

.forabg {
background-color: black !important;
}

ul.topiclist li.row dl {
border: 1px solid white;
box-shadow: 0px 0px 10px 2px rgb(153, 153, 153);
margin-bottom: 20px !important;
}

li.row:hover dl:hover {
background-color: #444 !important;
}

.row.bg1 {
margin-bottom: unset !important;
}

.row.bg2 {
margin-bottom: unset !important;
}








/* v1.3 Funktionen! */


.reply-icon span {
background-image: url(https://i.ibb.co/yXKpNsq/forum-antworten.png);
background-size: cover;
}

.quote-icon, .quote-icon a {
background-image: url(https://i.ibb.co/hLdkp71/forum-zitieren.png);
background-size: cover;
}

[style="background-image: url(./styles/prosilver/imageset/forum_read.gif); background-repeat: no-repeat;"] {
background-image: url(https://i.ibb.co/d4PLhNB/forum-gelesen.png) !important;
background-size: 30px 30px;
}

[style="background-image: url(./styles/prosilver/imageset/topic_read.gif); background-repeat: no-repeat;"] {
background-image: url(https://i.ibb.co/d4PLhNB/forum-gelesen.png) !important;
background-size: 30px 30px;
}

p.author {
color: #999 !important;
}

.post:target .content {
color: white !important;
}

#navigation a {
    background: #444 !important;
}

#navigation #active-subsection a {
    color: #D31141 !important;
    background-color: #777 !important;
}

.cp-mini {
    background-color: #555 !important;
}

#cp-main .panel {
    background-color: #555 !important;
    border: none !important;
    box-shadow: none !important;
}

#tabs a span {
    color: #fff;
    background: #999 !important;
    transition: 0.5s background;
}

#tabs a {
    background: #999 !important;
    transition: 0.5s background;
}

#tabs a:hover span {
    color: #BC2A4D;
    background: #bbb !important;
}

#tabs a:hover {
    background: #bbb !important;
}

.inputbox:focus {
    color: white !important;
}

.inputbox {
    background-color: black !important;
    color: white !important;
}

select {
    background-color: #222 !important;
    color: white !important;
}

.icon-members {
    background-image: url(https://i.ibb.co/T0VXqKv/icon-members.png);
    background-size: 16px;
}

.icon-logout {
    background-image: url(https://i.ibb.co/SNcZyvv/icon-logout.png);
    background-size: 16px;
}

.icon-ucp {
    background-image: url(https://i.ibb.co/803s8kg/icon-ucp.png);
    background-size: 16px;
}

.icon-home {
    background-image: url(https://i.ibb.co/rfsdbhg/icon-home.png);
    background-size: 13px;
}

.icon-faq {
    background-image: url(https://i.ibb.co/ZW0vdjj/icon-faq.png);
    background-size: 16px;
}

.icon-thanks_toplist {
    background-image: url(https://i.ibb.co/TL2mvQg/icon-thanks-toplist.png);
    background-size: 15px;
}

.online {
    background-image: url(https://i.ibb.co/9W030CF/icon-user-online.png);
    background-size: 58px;
}










/* v1.4 Animationen! */

dl.icon {
transition: background-color 0.5s;
background-color: black;
}
icon:hover {
background-color: #444 !important;
transition-delay: 0.1s;
}

a {
transition: color 0.2s;
}
