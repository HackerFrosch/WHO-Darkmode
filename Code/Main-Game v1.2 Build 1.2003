	font-size:20px;
	font-weight:normal;
	border-bottom:1px solid #aaa;
}
.content {
	font-size:16px;
	text-align:left;
	position:absolute;
	left:170px;
	top:190px;
	width:890px;
	height:560px;
        overflow:auto;
	background:transparent;
	font-weight:normal;
        padding-bottom: 40px;
}
a:link, a:visited {
        color: #9d2032;
        text-decoration: none;
	transition: color 0.5s;
}
a:hover, a:active {
        text-decoration: none;
	color: #ff3535;
}
tr.zeile_g {
	background-color: #424242;
	transition: background 1s, color 0.5s;
	text-shadow: 1px 1px 1px #121212;
}
tr.zeile_u {
	transition: background 1s;
	background: #121212 !important;
	color: #fff !important;
}
