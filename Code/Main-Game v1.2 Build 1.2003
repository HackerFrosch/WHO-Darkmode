
* {
	margin:0;
	padding:0;
}
html {
	background: #121212;
	background-size: 100%;
	background-size: cover;
	height: 100%;
}
body {
	font-family: "Source Sans Pro", "Trebuchet MS", Tahoma, Helvetica, sans-serif;
	font-size:16px;
	color:#F5F5F5;
}

ul.m_menu0 {
	list-style:none;
	position:relative;
	margin:0px 0px 0px 0px;
	width:890px;
	float:left;
	clear:both;
	z-index:0;
}
ul.m_menu0 > li {
	width:110px;
	height:101px;
	padding-bottom:2px;
	float:left;
	position:relative;
	overflow: hidden;
	border-radius: 2px;
}
ul.m_menu0 > li > a {
	display:block;
	background-color: #9d2032;
	color:#fff;
	text-decoration:none;
	font-size:16px;
	width:105px;
	height:95px;
	padding:10px;
	margin:4px;
	transition: background 1s, color 0.5s;
	border-radius: 2px;
	box-shadow: 0px 0px 5px 0px rgb(0, 0, 0);
	box-sizing: border-box;
	border: 1px solid rgba(255,255,255,0.16);
}
ul.m_menu0 > li > a:hover {
	background:#ebb5be;
	color:#121212;
	text-shadow: 1px 1px 1px #616161;
}
ul.m_menu0 > li > p {
	display:block;
	background:none;
	color:#fff;
	text-decoration:none;
	font-size:16px;
	width:90px;
	height:80px;
	padding:10px;
	margin:2px;
}
ul.m_menu0 > li > a.disabled {
	opacity:0.3;
}
ul.m_menu1 {
	list-style:none;
	position:relative;
	margin:0px 0px 0px 0px;
	width:890px;
	float:left;
	clear:both;
	z-index:0;
}
ul.m_menu1 > li {
	width:110px;
	height:101px;
	padding-bottom:2px;
	float:left;
	position:relative;
	overflow: hidden;
}
ul.m_menu1 > li > a {
	display:block;
	color:#fff;
	text-decoration:none;
	font-size:16px;
	width:105px;
	height:95px;
	padding:10px;
	margin:4px;
	background-color:rgba(255,255,255,0.15);
	transition: background 1s, color 0.5s;
	box-sizing: border-box;
	box-shadow: 0px 0px 5px 1px #000;
	border: 1px solid rgba(255,255,255,0.16);
	text-shadow: 1px 1px 1px #121212;
	border-radius: 2px;
}
ul.m_menu1 > li > a:hover {
	background:rgba(255,255,255,0.75);
	color:#121212;
	text-shadow: 1px 1px 1px #616161;
}
ul.m_menu1 > li > p {
	display:block;
	background:none;
	color:#fff;
	text-decoration:none;
	font-size:16px;
	width:90px;
	height:80px;
	padding:10px;
	margin:2px;
}
ul.m_menu1 > li > a.disabled {
	opacity:0.3;
	border: 1px solid rgba(255,255,255,0.12);
	color: rgb(255,255,255,0.38);
	background: rgba(255,255,255,0.12);
	text-shadow: none !important;
}
@keyframes message {
	0% { background-color:#f0a0a0; }
	50% { background-color:#f0f0f0; }
	100% { background-color:#f0a0a0; }
}

ul.arrList { 
	margin-left:0em; 
	padding-left:0.5em; 
	margin-top:1em; 
	margin-bottom:1em; 
}
ul.arrList li { 
	background: url('../icn/rarr.png') -0.3em -0.3em no-repeat;
	padding-left: 2em; 
	padding-bottom: 1em; 
	list-style: none; 
}

ul.oe_menu{
	list-style:none;
	position:relative;
	margin:30px 0px 0px 40px;
	width:560px;
	height:101px;
	float:left;
	clear:both;
	z-index:10;
}
ul.oe_menu > li{
	width:112px;
	height:101px;
	padding-bottom:2px;
	float:left;
	position:relative;
}
ul.oe_menu > li > a{
	display:block;
	background-color: rgba(255,255,255,0.15);
	color: rgba(255,255,255,0.87);
	text-decoration:none;
	border-radius: 2px;
	font-size:16px;
	width:105px;
	height:95px;
	padding:10px;
	margin:4px;
	box-sizing: border-box;
	transition: background 1s, color 0.5s;
	box-shadow: 0px 0px 5px 0px rgba(0,0,0,1);
	border: 1px solid rgba(255,255,255,0.16);
}
ul.oe_menu > li > a:hover,
ul.oe_menu > li.selected > a{
	background: rgba(255,255,255,0.75);
	color: #121212;
}
.oe_wrapper ul.hovered > li > a{
	background:#212121; 
	border:1px solid rgba(255,255,255,0.5);
}
ul.oe_menu div{
	position:absolute;
	top:53px;
	left:1px;
	background:#212121;  
	border:1px solid rgba(255,255,255,0.5);
	width:482px;
	min-height:175px;
	padding:20px;
	display:none;
	text-shadow: 1px 1px 1px #121212;
	border-radius: 2px;
}
ul.oe_menu div ul li a{
	text-decoration:none;
	color:#fff;
	padding:2px 2px 2px 2px;
	margin:2px;
	display:block;
	transition: background 0.5s, color 0.5s;
	font-size:14px;
	border-radius: 5px;
}
ul.oe_menu div ul.oe_full{
	width:100%;
}
ul.oe_menu div ul li a:hover{
	background:#424242;
	color:#fff;
}
ul.oe_menu li ul{
	list-style:none;
	float:left;
	width:150px;
	margin-right:10px;
}
li.oe_heading{
	color:#fff;
	font-size:16px;
	margin-bottom:10px;
	margin-right:10px;
	padding-bottom:6px;
	border-bottom:1px solid #aaa;
}

ul.oe_sidebar{
	list-style:none;
	position:relative;
	margin:0px 0px 0px 40px;
	width:112px;
	float:left;
	clear:both;
    padding-bottom: 50px;
}
ul.oe_sidebar > li{
	width:112px;
	height:101px;
	padding-bottom:2px;
	float:left;
	position:relative;
}
ul.oe_sidebar > li > a{
	display:block;
	background-color:#9d2032;
	color:#fff;
	text-decoration:none;
	font-size:16px;
	width:105px;
	height:95px;
	padding:10px;
	margin-left:4px !important;
	margin-top: 4px !important;
	box-sizing: border-box;
	box-shadow: 0px 0px 5px 0px rgba(0,0,0,1);
	border: 1px solid rgba(255,255,255,0.16);
	border-radius: 2px;
	min-height: 60px;
}
ul.oe_sidebar > li > a:hover,
ul.oe_sidebar > li.selected > a{
	background:#ebb5be;
	color: #121212;
	text-shadow: 1px 1px 1px #616161;
}
ul.oe_sidebar div{
	position:absolute;
	top:1px;
	left:113px;
	background:#ebb5be;
	border-right:1px solid #9d2032;
	min-width:292px;
	min-height:180px;
	padding:20px;
	display:none;
	z-index:9;
}
.bg_img img{
	width:100%;
	position:fixed;
	top:0px;
	left:0px;
	z-index:-1;
}
.menu_logo{
	font-size:64px;
	text-align:right;
	position:absolute;
	left:818px;
	top:30px;
	height:101px;
	width:240px;
	font-weight:normal;
}
