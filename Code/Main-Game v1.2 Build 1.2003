/* Chat-Design */

.message-right {
  background-color: #424242 !important;
	color: #F5F5F5 !important;
	border: unset !important;
	box-shadow: 0px 0px 10px 0px #000 !important;
}
.speaker-right {
	background: #757575 !important;
	color: #F5F5F5 !important;
	box-shadow: 5px 0px 10px 0px #000 !important;
}
.triangle-right {
	border-left: 20px solid #757575 !important;
}
.message-right.lehrer {
  background: #0D47A1 !important;
  border: unset !important;
  color: #F5F5F5 !important;
  box-shadow: 0px 0px 10px 0px #000 !important;
}
.speaker-right.lehrer {
  background-color: #1976D2 !important;
  color: #F5F5F5 !important;
  box-shadow: 5px 0px 10px 0px #000 !important;
}
.triangle-right.lehrer {
  border-left: 20px solid #1976D2 !important;
}
.triangle-left {
  border-right: 20px solid #00E676 !important;
}
.speaker-left {
	background: #00E676 !important;
	color: #121212 !important;
	box-shadow: 0px 0px 10px 0px #000 !important;
}
.message-left {
	background: #00C853 !important;
	border: unset !important;
	box-shadow: 10px 0px 10px 0px #000 !important;
}
.message-new {
    background-color: #424242 !important;
}
[name="msgtext"] {
	background: #424242 !important;
	color: #fff !important;
}
#charCounter {
	color: #fff !important;
}
div#months {
    background-color: unset !important;
}





